{
  "name": "pa-accounts-service",
  "author": "121.global",
  "homepage": "https://www.121.global/",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "prestart": "npm run migration:run",
    "start": "node index.js",
    "start:dev": "tsc-watch --onSuccess \"  node index.js  \" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "test": "jest --config=jest.json",
    "test:watch": "npm test -- --watchAll",
    "test:coverage": "npm test -- --coverage --coverageDirectory=coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run"
  },
  "private": true,
  "dependencies": {
    "@godaddy/terminus": "^4.1.0",
    "@nestjs/common": "^6.0.1",
    "@nestjs/core": "^6.0.1",
    "@nestjs/platform-express": "^6.0.1",
    "@nestjs/swagger": "^3.0.1",
    "@nestjs/terminus": "^6.1.4",
    "@nestjs/testing": "^6.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "crypto": "^1.0.1",
    "cryptr": "^6.0.1",
    "jsonwebtoken": "^8.1.1",
    "pg": "^7.9.0",
    "rxjs": "^6.4.0",
    "tsc-watch": "^2.1.2",
    "typeorm": "^0.2.15",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "10.x",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.18.2",
    "swagger-ui-express": "^4.0.6",
    "ts-jest": "^24.0.0",
    "ts-node": "~8.3.0",
    "tsconfig-paths": "^3.9.0"
  }
}
